{
  "name": "ssr-react-starter",
  "version": "1.0.0",
  "description": "Serverside Rendering React App",
  "main": "index.js",
  "repository": "git@github.com:I-am-aar/ssr-react-starter.git",
  "author": "AAR <aar@aar.is>",
  "license": "MIT",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch build --exec \"node build/bundle.js\"",
    "dev:build-server": "webpack --mode development --config webpack.server.js --watch",
    "dev:build-client": "webpack --mode development --config webpack.client.js --watch",
    "start": "node build/bundle.js",
    "prod:build-server": "webpack --config webpack.server.js",
    "prod:build-client": "webpack --config webpack.client.js",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm run prod:build-server && npm run prod:build-client"
  },
  "dependencies": {
    "express": "^4.17.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "react-router-config": "^5.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-es2017": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-stage-0": "^7.8.3",
    "axios": "^0.19.2",
    "babel-loader": "^8.1.0",
    "express-http-proxy": "^1.6.0",
    "lodash": "^4.17.15",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "react-helmet": "^6.0.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  }
}
